{
  "name": "bitlyexercise",
  "version": "1.0.0",
  "description": "Exercise for bitly EU",
  "main": "index.js",
  "scripts": {
    "smokeTest": "cypress run --spec \"cypress/e2e/features/smoke.feature\" --config viewportWidth=1920,viewportHeight=1080",
    "regression": "cypress run --e2e --config viewportWidth=1920,viewportHeight=1080",
    "mobileRegression":"cypress run --e2e --browser chrome --config viewportWidth=412,viewportHeight=915"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuanMunoz93/bitlyExercise.git"
  },
  "author": "Juan Camilo Munoz Bedoya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JuanMunoz93/bitlyExercise/issues"
  },
  "homepage": "https://github.com/JuanMunoz93/bitlyExercise#readme",
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "cypress/e2e/stepDefinitions/*.js"
    ],
    "html": {
      "enabled": true
    }
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "15.1.4",
    "@bahmutov/cypress-esbuild-preprocessor": "2.2.0",
    "@cypress/xpath": "2.0.3",
    "@faker-js/faker": "^7.6.0",
    "cypress": "12.7.0",
    "jimp": "^0.22.7",
    "qrcode-reader": "^1.0.4"
  }
}
